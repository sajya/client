{
    "name": "sajya/client",
    "type": "library",
    "description": "HTTP client and server for JSON-RPC 2.0",
    "keywords": [
        "rpc",
        "json-prc",
        "api"
    ],
    "homepage": "https://sajya.github.io",
    "license": "MIT",
    "authors": [
        {
            "name": "tabuna",
            "email": "bliz48rus@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "sajya/server": "^5.2|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sajya\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sajya\\Client\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
